
@{
    ViewData["Title"] = "Products";
}
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name
            </th>
            <th>
                Description
            </th>
            <th>
                Quantity
            </th>
            <th>
                 Price
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var unitOfMeasures in Model)
        {
            <tr>
                <td>
                    @unitOfMeasures.Id
                </td>
                <td>
                    @unitOfMeasures.Name
                </td>
                <td>
                    @unitOfMeasures.Description
                </td>
                <td>
                    @unitOfMeasures.Quantity
                </td>
                <td>
                    @unitOfMeasures.Price
                </td>
            </tr>
        }
    </tbody>

